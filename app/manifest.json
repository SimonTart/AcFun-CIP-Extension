{
  "manifest_version": 2,
  "name": "AcFun-CIP",
  "description": "AcFun Comment Instrumentality Project Extension（A站评论补全计划插件)",
  "version": "2.2.0",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "content_scripts": [{
    "matches": ["*://www.acfun.cn/a/*","*://www.acfun.cn/v/*"],
    "js": ["./content_script.js"]
  }],
  "permissions": [
    "*://*.trisolar.top/*"
  ]
}